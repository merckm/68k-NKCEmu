/************************************************************************/
/*									*/
/*	BIOS Interface to keyboard functions				*/
/*									*/
/************************************************************************/

/************************************************************************/
/*	VME-10 Keyboard Input						*/
/************************************************************************/

BYTE	cons_in()
{
	while( ! cons_stat() )		/* wait for input */
		;

	char_avail = 0;
	return( in_char );
}
