			/*address of 1st charactr*/
			p1 = &(SCRN_ADDR[line++][scrn_tbl.base_col]);
			p2 = &(SCRN_ADDR[line][scrn_tbl.base_col]);
			do {		/*move a line at a time	*/
				*p1++ = *p2++;	/*move character up a line */
